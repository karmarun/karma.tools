{
  "name": "@karma.run/editor-server",
  "version": "0.15.0",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": "github:karmarun/karma.tools",
  "scripts": {
    "build": "run-s copy-license sync-package-info build:*",
    "build:cjs": "tsc --declaration --module commonjs --outDir ./lib",
    "watch": "npm-run-all copy-license sync-package-info watch:cjs",
    "watch:cjs": "tsc -p ./tsconfig.json --declarationMap --declaration --module commonjs --outDir ./lib --watch",
    "copy-license": "shx cp ../../LICENSE ./",
    "clean": "shx rm -rf ./LICENSE ./lib",
    "prepare": "run-s clean copy:license build sync-package-info",
    "sync-package-info": "node ./../../scripts/syncPackageInfo.js ./src/version.ts"
  },
  "dependencies": {
    "@karma.run/sdk": "^0.6.0",
    "express-http-proxy": "^1.5.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/express-http-proxy": "^1.5.10",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "express": "^4.16.4"
  }
}
