{
  "name": "@karma.run/editor",
  "version": "0.13.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "Administrative editor for karma.data.",
  "bin": {
    "karma-editor": "./dist/cli/index.js"
  },
  "license": "MIT",
  "repository": "github:karmarun/karma.tools",
  "scripts": {
    "start": "node ./dist/index.js server",
    "cli": "node ./dist/index.js",
    "watch": "npm-run-all build --parallel watch:*",
    "build": "tsc",
    "watch:build": "tsc --watch",
    "watch:run": "nodemon --watch \"./dist/**/*\" --exec \"node ./dist/index.js server --watch\"",
    "copy:license": "shx cp ../../LICENSE ./",
    "clean": "shx rm -rf ./LICENSE ./dist",
    "prepare": "run-s clean copy:license build"
  },
  "dependencies": {
    "@karma.run/editor-server": "^0.13.0",
    "@types/findup-sync": "^0.3.29",
    "commander": "^2.15.1",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "findup-sync": "^2.0.0",
    "interpret": "^1.1.0",
    "liftoff": "^2.5.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  },
  "optionalDependencies": {
    "@karma.run/editor-client": "^0.13.0",
    "@karma.run/editor-worker": "^0.13.0",
    "babel-polyfill": "^6.26.0",
    "intersection-observer-polyfill": "^0.1.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1",
    "webpack": "^4.12.0"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/express": "^4.11.1",
    "@types/mkdirp": "^0.5.2",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.1",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3",
    "raven-js": "^3.25.1",
    "shx": "^0.3.0"
  }
}
